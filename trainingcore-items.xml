<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="items.xsd">

    <collectiontypes>
        <collectiontype code="GenderList" elementtype="Gender" autocreate="true" generate="true" type="list"/>
        <collectiontype code="SwatchColorSet" elementtype="SwatchColorEnum" autocreate="true" generate="true"
                        type="set"/>
        <collectiontype code="SkillList" elementtype="java.lang.String" autocreate="true" generate="true" type="list"/>
    </collectiontypes>

    <enumtypes>
        <enumtype generate="true" code="SwatchColorEnum" autocreate="true" dynamic="true">
            <value code="BLACK"/>
            <value code="BLUE"/>
            <value code="BROWN"/>
            <value code="GREEN"/>
            <value code="GREY"/>
            <value code="ORANGE"/>
            <value code="PINK"/>
            <value code="PURPLE"/>
            <value code="RED"/>
            <value code="SILVER"/>
            <value code="WHITE"/>
            <value code="YELLOW"/>
        </enumtype>
        <enumtype generate="true" autocreate="true" code="postStatus">
            <value code="PUBLISHED"/>
            <value code="DRAFT"/>
            <value code="PENDING"/>
            <value code="TRASH"/>
        </enumtype>

        <enumtype generate="true" autocreate="true" code="commentStatus">
            <value code="APPROVE"/>
            <value code="REJECT"/>
            <value code="PENDING"/>
            <value code="TRASH"/>
        </enumtype>

        <enumtype generate="true" autocreate="true" code="empGender">
            <value code="MALE" />
            <value code="FEMALE"/>
            <value code="OTHER"/>
        </enumtype>

        <enumtype generate="true" autocreate="true" code="empRole">
            <value code="ADMIN" />
            <value code="FRONTEND" />
            <value code="BACKEND" />
            <value code="DATABASE" />

        </enumtype>
    </enumtypes>

    <relations>
        <relation code="Blog2Posts" generate="true" localized="false" autocreate="true">
            <sourceElement type="blog" cardinality="one" qualifier="blog">
                <modifiers read="true" write="true" search="true" optional="false"/>
            </sourceElement>
            <targetElement type="posts" cardinality="many" qualifier="posts" ordered="true">
                <modifiers read="true" write="true" search="true" optional="true" partof="true"/>
            </targetElement>
        </relation>
        <relation code="Posts2Comments" generate="true" localized="false" autocreate="true">
            <sourceElement type="posts" cardinality="one" qualifier="posts">
                <modifiers read="true" write="true" search="true" optional="false"/>
            </sourceElement>
            <targetElement type="postComments" cardinality="many" qualifier="postComments" ordered="true">
                <modifiers read="true" write="true" search="true" optional="true" partof="true"/>
            </targetElement>
        </relation>

    </relations>

    <itemtypes>

        <!--         Add your item definitions here -->



        <itemtype code="blog"
                  autocreate="true"
                  generate="true">
            <deployment table="blog" typecode="11001"/>
            <attributes>
                <attribute qualifier="name" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="active" type="java.lang.Boolean">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="posts"
                  autocreate="true"
                  generate="true">
            <deployment table="post" typecode="11002"/>
            <attributes>
                <attribute qualifier="pid" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="author" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="title" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="content" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="Keywords" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="status" type="postStatus">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="postComments"
                  autocreate="true"
                  generate="true">
            <deployment table="postComments" typecode="11003"/>
            <attributes>
                <attribute qualifier="author" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="content" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="status" type="commentStatus">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="DummyBlog"
                  autocreate="true"
                  generate="true">
            <deployment table="dummy" typecode="11006"/>
            <attributes>
                <attribute qualifier="pid" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="author" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="title" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="content" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="keywords" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="status" type="postStatus">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="CustomTestComponent" autocreate="true" generate="true" extends="SimpleCMSComponent"
                  jaloclass="com.training.core.jalo.contents.components.CustomTestComponent">
            <attributes>
                <attribute type="localized:Media" qualifier="image">
                    <description>Image for the component</description>
                    <persistence type="property" />
                </attribute>
                <attribute type="localized:java.lang.String" qualifier="description">
                    <description>Description of the image</description>
                    <persistence type="property" />
                </attribute>
                <attribute type="localized:java.lang.String" qualifier="link">
                    <description>Link for new page</description>
                    <persistence type="property" />
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="CustomCarouselComponent" autocreate="true"
                  generate="true" extends="SimpleCMSComponent"
                  jaloclass="com.training.core.jalo.CustomCarouselComponent">
            <description>Cusom component for carousel.</description>
        </itemtype>

        <itemtype code="enquiryForm"
                  autocreate="true"
                  generate="true">
            <deployment table="enquiryForm" typecode="11089"/>
            <attributes>
                <attribute qualifier="firstName" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="lastName" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="age" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>
                <attribute qualifier="email" type="java.lang.String">
                    <persistence type="property"/>
                    <modifiers read="true" write="true"/>
                </attribute>

            </attributes>
        </itemtype>

        <!--Employee Component -->
<itemtype code="EmployeeComponent"
          autocreate="true"
          generate="true">
    <deployment table="EmployeeComponent" typecode="20221"/>
    <attributes>
        <attribute qualifier="empId" type="java.lang.Integer">
            <persistence type="property"/>
            <modifiers read="true" write="true" unique="true"/>
        </attribute>

        <attribute qualifier="empFname" type="java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empLname" type="java.lang.String">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empAge" type="java.lang.Integer">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empSalary" type="java.lang.Integer">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empGender" type="empGender">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empSkills" type="SkillList">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

        <attribute qualifier="empRole" type="empRole">
            <persistence type="property"/>
            <modifiers read="true" write="true"/>
        </attribute>

    </attributes>



</itemtype>

<itemtype code="ProductsRemovalCronjob" extends="CronJob" autocreate="true" generate="true">
    <attributes>
        <attribute qualifier="xDaysOld" type="java.lang.Integer">
            <modifiers read="true" write="true" optional="false"/>
            <defaultvalue>Integer.valueOf(10)</defaultvalue>
            <persistence type="property"/>
        </attribute>
    </attributes>
</itemtype>














        <!-- TypeGroups are for structure of this file only -->

        <typegroup name="Apparel">
            <itemtype code="ApparelProduct" extends="Product"
                      autocreate="true" generate="true"
                      jaloclass="org.training.core.jalo.ApparelProduct">
                <description>Base apparel product extension that contains additional attributes.</description>
                <attributes>
                    <attribute qualifier="genders" type="GenderList">
                        <description>List of genders that the ApparelProduct is designed for</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>

            <itemtype code="ApparelStyleVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="org.training.core.jalo.ApparelStyleVariantProduct">
                <description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
                <attributes>
                    <attribute qualifier="style" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color/Pattern of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>

                    <attribute qualifier="swatchColors" type="SwatchColorSet">
                        <description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>

            </itemtype>

            <itemtype code="ApparelSizeVariantProduct" extends="ApparelStyleVariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="org.training.core.jalo.ApparelSizeVariantProduct">
                <description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
                <attributes>
                    <attribute qualifier="size" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Size of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>

        <typegroup name="Electronics">
            <itemtype code="ElectronicsColorVariantProduct" extends="VariantProduct"
                      autocreate="true" generate="true"
                      jaloclass="org.training.core.jalo.ElectronicsColorVariantProduct">
                <description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
                <attributes>
                    <attribute qualifier="color" type="localized:java.lang.String"
                               metatype="VariantAttributeDescriptor">
                        <description>Color of the product.</description>
                        <modifiers/>
                        <persistence type="property"/>
                    </attribute>
                </attributes>
            </itemtype>
        </typegroup>



    </itemtypes>
</items>




